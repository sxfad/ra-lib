{
    "scripts": {
        "init": "rm -rf node_modules && npm install && npm run clean && npm run bootstrap && npm run build",
        "bootstrap": "lerna bootstrap",
        "clean": "lerna clean --yes",
        "start": "node scripts/build-watch.js",
        "build": "father-build",
        "build:config-loader": "PACKAGE=config-loader father-build",
        "build:admin": "PACKAGE=admin father-build",
        "build:hoc": "PACKAGE=hoc father-build",
        "build:model": "PACKAGE=model father-build",
        "build:hooks": "PACKAGE=hooks father-build",
        "build:ajax": "PACKAGE=ajax father-build",
        "build:babel-plugin-attribute-wrapper": "PACKAGE=babel-plugin-attribute-wrapper father-build",
        "build:util": "PACKAGE=util father-build",
        "build:components": "PACKAGE=components father-build",
        "build:component": "PACKAGE=component father-build",
        "build:admin-util": "PACKAGE=admin-util father-build",
        "build:options": "PACKAGE=options father-build",
        "publish": "lerna publish",
        "publish:yes": "lerna publish --yes",
        "publish:all": "sh scripts/publish.sh",
        "release": "npm run build && npm run publish",
        "version": "lerna ls",
        "dev": "dumi dev",
        "docs:build": "dumi build",
        "docs:deploy": "gh-pages -d docs-dist",
        "deploy": "npm run docs:build && npm run docs:deploy",
        "prettier": "prettier --write \"**/*.{js,jsx,tsx,ts,less,md,json}\"",
        "test": "umi-test",
        "test:watch": "umi-test --watch",
        "test:update": "umi-test --updateSnapshot",
        "test:coverage": "umi-test --coverage"
    },
    "main": "dist/index.js",
    "module": "dist/index.esm.js",
    "typings": "dist/index.d.ts",
    "lint-staged": {
        "*.{js,jsx,less,md,json}": [
            "prettier --write"
        ],
        "*.ts?(x)": [
            "prettier --parser=typescript --write"
        ]
    },
    "devDependencies": {
        "@ant-design/icons": "^4.6.2",
        "@testing-library/react": "^11.2.6",
        "@testing-library/react-hooks": "^5.1.1",
        "@types/jest": "^26.0.22",
        "@types/qs": "^6.9.7",
        "@umijs/fabric": "^2.5.7",
        "@umijs/test": "^3.4.8",
        "antd": "^4.15.1",
        "axios": "^0.21.1",
        "babel-plugin-import": "^1.13.3",
        "chalk": "^4.1.0",
        "chokidar": "^3.5.1",
        "classnames": "^2.3.1",
        "cross-env": "^7.0.3",
        "dumi": "^1.1.12",
        "eslint-config-prettier": "^8.1.0",
        "eslint-plugin-prettier": "^3.3.1",
        "father-build": "^1.19.6",
        "gh-pages": "^3.1.0",
        "globby": "^11.0.3",
        "lerna": "^4.0.0",
        "lint-staged": "^10.5.4",
        "lodash": "^4.17.21",
        "nodemon": "^2.0.7",
        "nprogress": "^0.2.0",
        "ora": "^5.4.0",
        "path-to-regexp": "^6.2.0",
        "ppfish": "^1.8.1",
        "prettier": "^2.2.1",
        "prop-types": "^15.7.2",
        "qs": "^6.10.1",
        "react": "^17.0.2",
        "react-redux": "^7.2.3",
        "react-sortable-hoc": "^2.0.0",
        "react-test-renderer": "^17.0.2",
        "redux": "^4.0.5",
        "redux-localstorage": "^0.4.1",
        "redux-thunk": "^2.3.0",
        "redux-undo": "^1.0.1",
        "shelljs": "^0.8.4",
        "yorkie": "^2.0.0"
    },
    "dependencies": {
        "jenkins": "^0.28.1"
    }
}
